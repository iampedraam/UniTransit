<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.unitransit.ReservationViewController"
            prefWidth="700" prefHeight="400">

    <TableView fx:id="reservationTable" layoutX="20" layoutY="20" prefWidth="660" prefHeight="300">
        <columns>
            <TableColumn fx:id="idColumn" text="ID"/>
            <TableColumn fx:id="studentColumn" text="Username"/>
            <TableColumn fx:id="fromColumn" text="From"/>
            <TableColumn fx:id="toColumn" text="To"/>
            <TableColumn fx:id="costColumn" text="Total Cost"/>
            <TableColumn fx:id="timeColumn" text="Reservation Time"/>

        </columns>
    </TableView>

    <Button text="Back" layoutX="20" layoutY="340" onAction="#onBackClick"/>
</AnchorPane>
